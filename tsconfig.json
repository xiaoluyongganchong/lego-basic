{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "jsx":"preserve",
    "skipLibCheck": true,
    "isolatedModules": true,
    "noImplicitAny": false ,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "types": ["vitest/globals","node"]
  },
  "include": ["src/**/*.ts", "src/**/*.vue", "tests/**/*.ts"]
}